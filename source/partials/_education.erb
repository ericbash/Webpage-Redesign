<table class="background">
    <tr>
        <th>Time Period</th>
        <th>Title</th>
        <th>Institution</th>
    </tr>

    <% last_time = ""%>
    <% data.background.Education.education.each do |item| %>
        <tr class="info">

        <% if item[0] == last_time %>
            <td></td>
        <% else %>
            <td><%= item[0] %></td>
            <% last_time = item[0] %>
        <% end %>

        <td><%= item[1] %></td>
        
        <% if item.size == 4 %>
            <td><%= link_to item[2], item[3] %></td>
        <% else%>
            <td><%= item[2] %><td>
        <% end %>

        </tr>
        
    <% end %>
</table>
